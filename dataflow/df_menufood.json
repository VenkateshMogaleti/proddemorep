{
	"name": "df_menufood",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DelimitedText1",
						"type": "DatasetReference"
					},
					"name": "menusource"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_csv",
						"type": "DatasetReference"
					},
					"name": "sink"
				},
				{
					"dataset": {
						"referenceName": "ds_csv",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "CatWiseRat"
				},
				{
					"name": "Newcolumn"
				},
				{
					"name": "aggregate1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          customerid as string,",
				"          customername as string,",
				"          salesperson as string,",
				"          hobbies as string,",
				"          modifydate as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> menusource",
				"menusource aggregate(Calories = ERROR_FUNCTION('')) ~> CatWiseRat",
				"menusource derive(weight = ERROR_FUNCTION('')) ~> Newcolumn",
				"Newcolumn aggregate() ~> aggregate1",
				"CatWiseRat sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          empid as string,",
				"          ename as string,",
				"          gender as string,",
				"          salary as string,",
				"          country as string",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink",
				"aggregate1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          empid as string,",
				"          ename as string,",
				"          gender as string,",
				"          salary as string,",
				"          country as string",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink1"
			]
		}
	}
}